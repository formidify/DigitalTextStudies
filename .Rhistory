library(tidyr)
library(Lahman)
glimpse(Teams)
cub <- Teams %>% filter(name == "Chicago Cubs") %>%
select(HR, HRA, yearID)
cub_narrow <- gather(cub, key = hit, value = hit_num, HR, HRA)
ggplot(cub_narrow) +
geom_line(aes(x = yearID, y = hit_num, color = hit)) +
ggtitle("Time series of home runs hit vs allowed")
help("geom_line")
